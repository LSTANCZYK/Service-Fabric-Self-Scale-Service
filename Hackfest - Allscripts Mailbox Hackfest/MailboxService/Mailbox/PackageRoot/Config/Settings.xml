<?xml version="1.0" encoding="utf-8" ?>
<Settings xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schemas.microsoft.com/2011/01/fabric">
  <!-- Add your custom configuration sections and parameters here -->
  
  <Section Name="DocumentDbConfig">
    <Parameter Name="EndpointUri" Value="[EndpointUri]" />
    <Parameter Name="AccountKey" IsEncrypted="true" Value="[secret]" />
    <Parameter Name="DatabaseId" Value="mailbox" />
    <Parameter Name="CollectionId" Value="mail" />
    <Parameter Name="TimeToLiveDefault" Value="604800" /> <!--7 days-->
  </Section>
</Settings>
